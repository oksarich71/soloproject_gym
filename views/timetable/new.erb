<form class="box" method="POST" action="/timetable/<%= @timetable.id %>">

    <label>
     Select a member:
      <select name="member_id">
       <% for member in @members %>
       <option value="<%= member.id %>"><%= member.name %></option>
       <% end %>
      </select>
    </label>

   <input type="submit" value="Add a member">

</form>
